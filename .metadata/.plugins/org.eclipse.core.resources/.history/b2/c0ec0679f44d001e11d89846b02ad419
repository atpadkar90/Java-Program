package Selenium;

public class SpcialCharacterandDigit {

	public static void main(String[] args) {

		String s="a+b&cd";
		String cha="";
		String spe="";
		spe=s.replaceAll("[a-z]","");
		
		for(int i=s.length()-1;i>=0;i--)
		{
		char ch=s.charAt(i);
		
		if(Character.isAlphabetic(ch))
		{
			cha=cha+ch;
		}
		
		}
	System.out.print(cha.substring(0,1)+spe.substring(0,1);
	}

}

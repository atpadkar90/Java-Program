	//Find out duplicate Charater
		
		
		
		String a="sagar Atppadkar";
		char []b=a.toCharArray();
		for(int i=0;i<b.length;i++)
		{
			for(int j=i+1;j<b.length;j++)
			{
				if(b[i]==b[j])
				{
					System.out.println(b[j]);
				}
			}
		}

	}

}
